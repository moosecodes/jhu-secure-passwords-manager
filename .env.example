# Secure Password Manager Environment Configuration
# Copy this file to .env and update the values

# Flask Web Application Settings
SPM_SECRET=generate-a-strong-secret-key-here
FLASK_ENV=development
FLASK_DEBUG=true

# Vault Configuration
VAULT_PATH=data/test.vault.json
DEFAULT_MASTER_PASSWORD=your-master-password-here

# Logging Configuration
LOG_LEVEL=INFO
LOG_DIR=logs

